{
  "name": "@alipay/o3-root",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "browser": "dist/index.browser.js",
  "types": "types/index.d.ts",
  "private": true,
  "bin": {
    "o3": "./scripts/create.module.js"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "tnpm i && lerna bootstrap",
    "test": "jest",
    "test-cov": "jest --coverage",
    "watch": "rollup -cw",
    "watch:umd": "cross-env NODE_ENV=BUILD rollup -cw",
    "ci": "lerna bootstrap --hoist && tnpm run build && tnpm run b:types && tnpm run test-cov",
    "lint": "eslint packages/*/src --ext .ts",
    "build": "rollup -c",
    "b:types": "lerna run b:types",
    "b:all": "npm-run-all --parallel b:types build && tnpm run b:umd && tnpm run b:miniprogram && tnpm run miniprogram-component",
    "b:doc": "node scripts/build.doc.js",
    "b:umd": "cross-env NODE_ENV=BUILD rollup -c",
    "b:playground": "cross-env NODE_ENV=production webpack --config playground/build/webpack.config.mpa.js",
    "b:playground-miniprogram": "node ./playground/build/build.miniprogram.js",
    "b:miniprogram": "cross-env NODE_ENV=MINIPROGRAM rollup -c",
    "b:miniprogram-component": "cd packages/component-miniprogram && tnpm run build",
    "c:module": "node scripts/create.module.js",
    "release": "lerna publish prerelease --yes",
    "release:recover": "lerna publish prerelease from-package --yes",
    "release:force": "lerna exec --no-bail -- tnpm publish --tag alpha",
    "webpack-dev-server": "cross-env NODE_ENV=development webpack-dev-server  --config playground/build/webpack.config.mpa.js",
    "dev": "npm-run-all --parallel watch webpack-dev-server",
    "clean": "lerna exec -- rm -rf dist",
    "docs:update": "node scripts/docs.update.js",
    "post-changelog": "node scripts/post-changelog.js",
    "fix-memory-limit": "cross-env LIMIT=8192 increase-memory-limit",
    "otp:release": "node scripts/release/index.js",
    "release:canary": "lerna publish prerelease --dist-tag alpha --yes --otp x"
  },
  "files": [
    "src",
    "types",
    "dist"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@ali/ci": "^4.24.0",
    "@ali/rollup-plugin-string": "^3.0.0",
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-transform-modules-umd": "^7.7.4",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.23",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "ali-contributors": "^2.2.2",
    "axios": "^0.19.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-external-helpers": "^6.22.0",
    "chalk": "^2.4.2",
    "copy-dir": "^1.1.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "husky": "^2.2.0",
    "increase-memory-limit": "^1.0.7",
    "inquirer": "^6.5.1",
    "jest": "^24.8.0",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^5.0.0",
    "lerna": "^3.13.4",
    "lint-staged": "^10.0.0-1",
    "lodash": "^4.17.11",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "portfinder": "^1.0.23",
    "prettier": "^1.18.2",
    "raw-loader": "^2.0.0",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-inject": "^3.0.1",
    "rollup-plugin-modify": "^3.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "typescript": "^3.7.3",
    "url-loader": "^1.1.2",
    "urllib": "^2.34.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "ci": {
    "type": "aci",
    "versions": "8, 10, 12"
  }
}
