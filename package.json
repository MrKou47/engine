{
  "name": "@alipay/r3-root",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "browser": "dist/index.browser.js",
  "types": "types/index.d.ts",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "npm install && lerna bootstrap",
    "test": "jest",
    "test-cov": "jest --coverage",
    "watch": "rollup -cw",
    "ci": "npm run lint && npm run test-cov",
    "lint": "tslint --project ./tsconfig.json",
    "lint:fix": "tslint --fix --project ./tsconfig.json",
    "build": "rollup -c",
    "b:types": "lerna run b:types",
    "b:all": "npm-run-all --parallel b:types build b:umd",
    "b:doc": "node scripts/build.doc.js",
    "b:umd": "cross-env NODE_ENV=BUILD rollup -c",
    "release": "lerna publish prerelease --yes",
    "release:recover": "lerna publish prerelease from-package --yes",
    "webpack-dev-server": "webpack-dev-server --config playground/build/webpack.config.mpa.js",
    "dev": "npm-run-all --parallel watch webpack-dev-server",
    "clean": "lerna exec -- rm -rf dist",
    "docs:update": "node scripts/docs.update.js",
    "post-changelog": "node scripts/post-changelog.js"
  },
  "files": [
    "src",
    "types",
    "dist"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@ali/rollup-plugin-string": "^3.0.0",
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-typescript": "^7.3.3",
    "axios": "^0.19.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-external-helpers": "^6.22.0",
    "chalk": "^2.4.2",
    "cross-env": "^5.2.0",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "husky": "^2.2.0",
    "inquirer": "^6.5.0",
    "jest": "^24.8.0",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^5.0.0",
    "lerna": "^3.13.4",
    "lodash": "^4.17.11",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^2.0.0",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "tslint": "^5.16.0",
    "typescript": "^3.5.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
