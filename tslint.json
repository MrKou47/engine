{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "indent": [
      true,
      "spaces",
      2
    ],
    "max-line-length": [false],
    "variable-name": {
      "options": [
        "allow-leading-underscore",
        "allow-pascal-case"
      ]
    },
    "no-bitwise": false,
    "ordered-imports": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ],
    "no-unused-expression": [true, "allow-fast-null-checks"],
    "arrow-parens": false,
    "no-console": true,
    "no-empty": false,
    "space-before-function-paren": false
  },
  "rulesDirectory": []
}
